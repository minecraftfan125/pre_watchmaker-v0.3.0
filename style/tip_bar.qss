/* tip_bar.qss - 提示列樣式 */

QWidget#tipBar {
    background-color: #252525;
    border-top: 1px solid #3d3d3d;
    min-height: 28px;
    max-height: 28px;
}

QLabel#tipLabel {
    color: #888888;
    font-size: 16px;
    padding-left: 10px;
    padding-right: 10px;
    background-color: transparent;
}
